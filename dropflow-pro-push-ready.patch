+  locale = "fr",
+}: {
+  title: string;
+  description: string;
+  locale?: "fr" | "en";
+}): Promise<SEOKeywordsResult> {
+  const prompt = `
+Tu es un expert en SEO pour e-commerce dropshipping.
+Analyse le produit suivant et g√©n√®re :
+1. Une liste de 10 mots-cl√©s optimis√©s
+2. Une meta-description de 160 caract√®res maximum
+3. Une liste de hashtags SEO-friendly
+
+Produit :
+Titre : ${title}
+Description : ${description}
+Langue : ${locale === "fr" ? "Fran√ßais" : "Anglais"}
+
+R√©ponds en JSON dans ce format :
+{
+  "keywords": [...],
+  "metaDescription": "...",
+  "hashtags": [...]
+}
+`;
+
+  const result = await generateCompletion(prompt, {
