+export const getLeadsByStatus = async (userId: string, status?: CRMLead['status']) => {
+  try {
+    let query = supabase
+      .from('crm_leads')
+      .select('*')
+      .eq('user_id', userId)
+      .order('created_at', { ascending: false });
