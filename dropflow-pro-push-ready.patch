+    """
+    now = datetime.utcnow()
+    diff = now - timestamp
+    
+    if diff.days > 0:
+        return f"{diff.days}d"
+    
+    hours = diff.seconds // 3600
+    if hours > 0:
+        return f"{hours}h"
+    
+    minutes = (diff.seconds % 3600) // 60
+    if minutes > 0:
