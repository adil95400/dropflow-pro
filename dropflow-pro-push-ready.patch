+export interface BigBuyOrder {
+  id: string;
+  reference: string;
+  status: string;
+  createdAt: string;
+  updatedAt: string;
+  shippingAddress: BigBuyAddress;
+  products: BigBuyOrderProduct[];
+  tracking?: {
+    carrier: string;
+    trackingNumber: string;
+    url: string;
+  };
+}
