+}
+
+export class WinnerDetector {
+  async analyzeProduct(productData: any): Promise<{
+    isWinner: boolean;
+    score: number;
+    analysis: string;
+    reasons: string[];
+  }> {
+    const prompt = `
+Tu es un expert en d√©tection de produits gagnants pour le dropshipping.
+
+Analyse ce produit et d√©termine s'il a le potentiel d'√™tre un "winner" :
+
+Produit :
+- Titre : ${productData.title}
+- Prix : ${productData.price}‚Ç¨
+- Cat√©gorie : ${productData.category}
+- Commandes : ${productData.orders || 0}
+- Note : ${productData.rating || 0}/5
+- Avis : ${productData.reviews || 0}
+
+Crit√®res d'√©valuation :
+1. Potentiel de marge (>100%)
+2. Demande du march√©
+3. Niveau de concurrence
+4. Tendance sociale
+5. Facilit√© de marketing
+6. Saisonnalit√©
+7. Probl√®me r√©solu
+8. Facteur "wow"
+
+R√©ponds en JSON :
+{
+  "isWinner": true/false,
+  "score": 85,
+  "analysis": "Analyse d√©taill√©e...",
+  "reasons": ["Raison 1", "Raison 2", ...]
+}
+`;
+
+    try {
+      const response = await openai.chat.completions.create({
+        model: 'gpt-4',
+        messages: [{ role: 'user', content: prompt }],
+        temperature: 0.7,
+        max_tokens: 800,
