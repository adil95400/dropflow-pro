+#!/bin/bash
+# DropFlow Pro - Pre-push Hook
+# This script runs before each push to ensure code quality
+
+# Exit on error
+set -e
+
+echo "üöÄ DropFlow Pre-push Hook"
+echo "------------------------"
+
+# Get the current branch
+CURRENT_BRANCH=$(git rev-parse --abbrev-ref HEAD)
+echo "üìÇ Current branch: $CURRENT_BRANCH"
+
+# Don't run on main/master branches
+if [[ "$CURRENT_BRANCH" == "main" || "$CURRENT_BRANCH" == "master" ]]; then
+    echo "‚ö†Ô∏è Pushing to $CURRENT_BRANCH branch. Running additional checks..."
+fi
+
+# Run full test suite
+echo "üß™ Running full test suite..."
+npm run test || {
+    echo "‚ùå Tests failed. Please fix the failing tests before pushing."
+    exit 1
+}
+
+# Run build to ensure it compiles
+echo "üî® Building project..."
+npm run build || {
+    echo "‚ùå Build failed. Please fix the build errors before pushing."
+    exit 1
+}
+
+# Run type checking
