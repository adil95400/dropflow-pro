+  dimensions: {
+    length: number;
+    width: number;
+    height: number;
+  };
+  stock: number;
+  ean: string;
+  attributes?: Record<string, string>;
+  variants?: BigBuyVariant[];
+}
+
+export interface BigBuyVariant {
+  id: string;
+  name: string;
+  price: number;
+  wholesalePrice: number;
+  stock: number;
+  sku: string;
+  attributes: Record<string, string>;
+}
